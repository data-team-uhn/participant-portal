{
  "name": "participant-portal-api",
  "version": "1.0.0",
  "license": "MIT",
  "description": "PPADS",
  "main": "src",
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": ">= 20.0.0",
    "yarn": ">= 0.18.0"
  },
  "scripts": {
    "test": "yarn run lint && yarn run compile && yarn run mocha",
    "lint": "eslint src/. test/. --config .eslintrc.json --ext .ts --fix",
    "dev": "yarn run clean && nodemon --ext .ts --exec \"node --inspect=0.0.0.0:9229 --require ts-node/register src/index.ts\"",
    "start": "yarn run compile --project tsconfig.build.json && node lib/",
    "mocha": "yarn run clean && npx mocha --inspect=0.0.0.0:9229 --require ts-node/register --require source-map-support/register \"test/**/*.ts\" test/ --recursive --exit || echo",
    "compile": "yarn run clean && tsc",
    "clean": "shx rm -rf lib/",
    "db:migrate": "npx sequelize db:migrate --migrations-path ./src/migrations --url postgresql://$POSTGRES_USER:$POSTGRES_PASSWORD@$POSTGRES_HOST/$POSTGRES_DB",
    "db:migrate:status": "npx sequelize db:migrate:status --migrations-path ./src/migrations --url postgresql://$POSTGRES_USER:$POSTGRES_PASSWORD@$POSTGRES_HOST/$POSTGRES_DB",
    "db:migrate:undo": "npx sequelize db:migrate:undo --migrations-path ./src/migrations --url postgresql://$POSTGRES_USER:$POSTGRES_PASSWORD@$POSTGRES_HOST/$POSTGRES_DB",
    "db:migrate:undo:all": "npx sequelize db:migrate:undo:all --migrations-path ./src/migrations --url postgresql://$POSTGRES_USER:$POSTGRES_PASSWORD@$POSTGRES_HOST/$POSTGRES_DB",
    "db:migrate:create": "npx sequelize migration:generate --migrations-path ./src/migrations"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "ignore": []
  },
  "types": "lib/",
  "dependencies": {
    "@azure/msal-node": "^3.8.0",
    "@faker-js/faker": "^9.6.0",
    "@feathersjs/authentication": "^5.0.33",
    "@feathersjs/authentication-local": "^5.0.33",
    "@feathersjs/authentication-oauth": "^5.0.33",
    "@feathersjs/configuration": "^5.0.33",
    "@feathersjs/errors": "^5.0.33",
    "@feathersjs/express": "^5.0.33",
    "@feathersjs/feathers": "^5.0.33",
    "@feathersjs/rest-client": "^5.0.33",
    "@feathersjs/socketio": "^5.0.33",
    "@types/node-localstorage": "^1.3.3",
    "axios": "^1.8.3",
    "bluebird": "^3.7.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "feathers-authentication-management": "^5.1.0",
    "feathers-hooks-common": "^8.2.1",
    "feathers-sequelize": "^7.0.3",
    "helmet": "^8.0.0",
    "isomorphic-fetch": "^3.0.0",
    "localstorage-memory": "^1.0.3",
    "mjml": "^4.15.3",
    "node-localstorage": "^3.0.5",
    "nodemailer": "^6.10.0",
    "nodemon": "^3.1.0",
    "pg": "^8.14.0",
    "qrcode": "^1.5.4",
    "sequelize": "^6.37.1",
    "serve-favicon": "^2.5.0",
    "typescript": "^5.8.2",
    "winston": "^3.12.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/assert": "^1.5.10",
    "@types/axios": "^0.14.4",
    "@types/bluebird": "^3.5.42",
    "@types/compression": "^1.7.5",
    "@types/isomorphic-fetch": "^0.0.39",
    "@types/lodash": "^4.17.7",
    "@types/mjml": "^4.7.4",
    "@types/mocha": "^10.0.7",
    "@types/node": "^22.13.10",
    "@types/nodemailer": "^6.4.17",
    "@types/qrcode": "^1.5.5",
    "@types/serve-favicon": "^2.5.7",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "eslint": "^9.22.0",
    "eslint-import-resolver-typescript": "^3.8.5",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-unused-imports": "^4.1.3",
    "mocha": "^11.1.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2"
  }
}
